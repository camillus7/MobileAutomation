public class MyWin_GoPaperlessPage {

	final WebDriver driver;
	private Validator vc;

	public MyWin_GoPaperlessPage(WebDriver driver) {
		this.driver = driver;
		this.vc = new Validator(driver);
	}
	
	
	@FindBy(xpath ="//span[contains(.,'Go Paperless')]")
	public WebElement goPaperlessOn;
	
	@FindBy(xpath ="//button[@class='btn btn-primary']")
	public WebElement okButton;
	
	@FindBy(xpath ="//button[@class='btn btn-primary']")
	public WebElement mailLink;
	
	@FindBy(xpath="//a[contains(.,'click here.')]")
	public WebElement clickHere;
	
	@FindBy(xpath="//button[@aria-hidden='true']")
	public WebElement crossButton;
	
	
	
	
	
	
	
	
	
	
