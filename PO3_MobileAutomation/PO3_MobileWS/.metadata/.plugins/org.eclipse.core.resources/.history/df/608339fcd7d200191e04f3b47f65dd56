package com.webautomation.pageobject.empPortal_pages;

import java.util.List;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.interactions.Actions;
import org.openqa.selenium.support.FindBy;

import com.webautomation.validation.Validator;

public class EmpPortal_SalesMarketingPage {

	final WebDriver driver;

	private Validator vc;

	public EmpPortal_SalesMarketingPage(WebDriver driver) {
		this.driver = driver;
		this.vc = new Validator(driver);
	}

	
	@FindBy(xpath="//a[@href='/Employees/Home/StartDemo']")
	public WebElement salesDemo;

	@FindBy(xpath="//a[contains(.,'Back to Client Tools')]")
	public WebElement backToClientTools;
	
	@FindBy(linkText="xxxxxxxxx")
	public WebElement xxxxx;

	

}	





